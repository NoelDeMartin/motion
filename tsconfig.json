{
  "extends": "./docs/.nuxt/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "csstype",
      "vitest",
      "vitest/globals",
      "vite/client"
    ],
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@vueuse/motion": [
        "./src/index"
      ],
      "@vueuse/motion/nuxt": [
        "./src/nuxt/module"
      ],
      "#app": [
        "./node_modules/nuxt/dist/app"
      ],
      "#app/*": [
        "./node_modules/nuxt/dist/app/*"
      ],
      "vue-demi": [
        "./node_modules/nuxt/dist/app/compat/vue-demi"
      ],
      "#color-mode-options": [
        "./docs/.nuxt/color-mode-options"
      ],
      "#nuxt-component-meta": [
        "./docs/.nuxt/component-meta.mjs"
      ],
      "#nuxt-component-meta/types": [
        "./docs/.nuxt/component-meta.d.ts"
      ],
      "#vue-router": [
        "./docs/.nuxt/vue-router"
      ],
      "#imports": [
        "./docs/.nuxt/imports"
      ],
      "#build": [
        "./docs/.nuxt"
      ],
      "#build/*": [
        "./docs/.nuxt/*"
      ],
      "@nuxt-themes/tokens/config": [
        "./node_modules/@nuxt-themes/tokens/dist/tokens.config.ts"
      ],
      "#pinceau/utils": [
        "./docs/.nuxt/pinceau/utils.ts"
      ],
      "#pinceau/theme": [
        "./docs/.nuxt/pinceau/index.ts"
      ],
      "#pinceau/schema": [
        "./docs/.nuxt/pinceau/schema.ts"
      ],
      "#pinceau/definitions": [
        "./docs/.nuxt/pinceau/definitions.ts"
      ],
      "#components": [
        ".nuxt/components"
      ]
    }
  },
  "include": [
    "./env.d.ts",
    "docs/**/*",
    "src/**/*",
    "tests/**/*",
    "docs/**/*",
    "playgrounds/**/*"
  ],
  "exclude": [
    "../dist",
    "../.output"
  ],
  "vueCompilerOptions": {
    "plugins": [
      "pinceau/volar"
    ]
  },
}
